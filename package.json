{
  "name": "educenter",
  "version": "1.0.0",
  "license": "MIT",
  "author": "cretm",
  "scripts": {
    "dev": "exec-bin node_modules/.bin/hugo server --bind=0.0.0.0 --disableFastRender --environment development",
    "tina": "tinacms dev --noTelemetry -c \"exec-bin node_modules/.bin/hugo server --bind=0.0.0.0 --disableFastRender\"",
    "build": "tinacms build && hugo --gc --minify",
    "build:local": "exec-bin node_modules/.bin/hugo hugo --gc --minify",
    "server": "exec-bin node_modules/.bin/hugo hugo server",
    "clean": "shx rm -rf public resources .hugo_build.lock",
    "clean:install": "shx rm -rf package-lock.json node_modules"
  },
  "devDependencies": {
    "@tinacms/cli": "^2.0.7",
    "@types/node": "^25.0.3",
    "exec-bin": "^1.0.0",
    "hugo-extended": "^0.147.9",
    "shx": "^0.3.3"
  },
  "dependencies": {
    "tinacms": "^3.2.0"
  }
}
